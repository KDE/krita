msgstr "Omogoči ali onemogoči filter »gaborish«."

#. i18n: ectx: property (text), widget (QLabel, lblGaborishFilter)
#: plugins/impex/jxl/kis_wdg_options_jpegxl.ui:498
#, kde-format
msgctxt "JPEG-XL encoder options"
msgid "Gaborish filter"
msgstr "Filter »gaborish«"

#. i18n: ectx: property (text), widget (QLabel, lbModularEncoding)
#: plugins/impex/jxl/kis_wdg_options_jpegxl.ui:505
#, kde-format
msgctxt "JPEG-XL encoder options"
msgid "Modular encoding"
msgstr "Modularno kodiranje"

